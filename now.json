{
  "version": 2,
  "build": {
    "env": {
      "FAUNADB": "@faunadb",
      "SENDGRID_EMAIL": "@sendgrid_email",
      "SENDGRID_API_KEY": "@sendgrid_api_key",
      "SENDGRID_TEMPLATE_REGISTER": "@sendgrid_template_register",
      "baseURL": "@base_url"
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
